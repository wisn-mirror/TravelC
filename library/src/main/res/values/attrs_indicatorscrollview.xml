<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndicatorScrollView">
        <!--图片大小-->
        <attr name="drawableSize" format="dimension"/>
        <!--图片之间的间隔-->
        <attr name="drawableMargin" format="dimension"/>
        <!--是否显示滑动过程-->
        <attr name="isScroll" format="boolean"/>
        <!--图片个数-->
        <attr name="scrollCount" format="integer"/>
        <!--滑动选择图片-->
        <attr name="drawableSelect" format="reference"/>
        <!--滑动背景图片-->
        <attr name="drawableUnSelect" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CountDownView">
        <!--颜色-->
        <attr name="ringColor" format="color" />
        <!-- 进度文本的字体大小 -->
        <attr name="progressTextSize" format="dimension" />
        <!-- 圆环宽度 -->
        <attr name="ringWidth" format="float" />
        <!--进度文本颜色-->
        <attr name="progressTextColor" format="color"/>
        <!-- 进度文本-->
        <attr name="progressText" format="dimension" />
        <!--倒计时-->
        <attr name="countdownTime" format="integer"/>
    </declare-styleable>
</resources>